page{
  background: #f5f5f5;
}
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
}
.index_tag{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.index_tag_left{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.index_icon{
  width: 6rpx;
  height: 32rpx;
  background: #fd6d01;
}
.index_title{
  font-size: 30rpx;
  color: #333333;
  font-weight: 500;
  margin-left: 15rpx;
}
.index_title_intro{
  color: #909090;
  font-weight: 500;
  font-size: 24rpx;
  margin-left: 28rpx;
  margin-top: 6rpx;
}
.index_more{
  width: 64rpx;
  height: 33rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border: #333333 solid 1rpx;
  border-radius: 16.5rpx;
  justify-content: center;
  color: #333333;
  font-size: 18rpx;
}
.index_more image{
  width:11rpx;
  height: 11rpx;
}
/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
::-webkit-scrollbar {
     display: none;
}
.index_tag{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.index_tag_left{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.index_icon{
  width: 6rpx;
  height: 32rpx;
  background: #fd6d01;
}
.index_title{
  font-size: 30rpx;
  color: #333333;
  font-weight: 500;
  margin-left: 15rpx;
}
.index_title_intro{
  color: #909090;
  font-weight: 500;
  font-size: 24rpx;
  margin-left: 28rpx;
  margin-top: 6rpx;
}
