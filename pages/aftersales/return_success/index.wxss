.return_success_top{
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 190rpx;
  background: linear-gradient(90deg,rgba(253,109,1,1),rgba(252,152,3,1));
}
.return_icon{
  width: 680rpx;
  margin-left: 70rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.return_icon image{
  width: 90rpx;
  height: 90rpx;
}
.return_icon text{
  color: #ffffff;
  font-size: 32rpx;
  margin-left: 20rpx;
  font-weight: 500;
}
.return_show{
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #ffffff;
}
.return_show_list{
  width: 690rpx;
  padding: 20rpx 30rpx;
  border-bottom: #f7f7f7 solid 1rpx;
}
.return_show_item{
  margin-top: 10rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}
.return_show_label{
  color: #000916;
  font-size: 26rpx;
  font-weight: 500;
}
.return_show_intro{
  color: #595959;
  font-size: 26rpx;
  font-weight: 500;
  width: 80%;
  margin-left: 30rpx;
  line-height: 30rpx;
}
.return_show_btn{
  width: 690rpx;
  padding: 20rpx 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.return_show_btn view,.return_show_btn navigator{
  width: 150rpx;
  height: 54rpx;
  line-height: 54rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  border-radius: 27rpx;
  border: #9c9c9c solid 1rpx;
  color: #595959;
  font-size: 24rpx;
}
.return_show_btn view.return_back{
  margin-right: 20rpx;
}
.return_show_btn navigator.return_active{
  border:#fd7712 solid 1rpx;
  color: #fd7712;
}
